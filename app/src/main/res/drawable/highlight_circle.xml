<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <!-- White inner fill -->
    <solid android:color="@android:color/white" />

    <!-- Thin black stroke -->
    <stroke
        android:width="1dp"
        android:color="@android:color/black" />

    <!-- Smooth edges -->
    <size
        android:width="60dp"
        android:height="60dp" />
</shape>